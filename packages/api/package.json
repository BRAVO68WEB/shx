		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.7.0",
		"graphqurl": "^1.0.1",
		"hasura-cli": "^2.29.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	}
