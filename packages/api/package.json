		"@aws-sdk/lib-storage": "^3.417.0",
		"@hono/node-server": "^1.2.0",
		"@hono/zod-validator": "^0.1.8",
		"axios": "^1.2.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"dotenv-cli": "^7.3.0",
		"form-data": "^4.0.0",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"hono": "^3.5.6",
		"joi": "^17.7.0",
		"napi-nanoid": "^0.2.0",
