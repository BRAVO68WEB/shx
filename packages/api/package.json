		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"graphqurl": "^1.0.1",
		"hasura-cli": "^2.31.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"nodemon": "^3.0.1",
		"prettier": "^2.8.2",
		"typescript": "^5.1.6",
		"wrangler": "^3.9.0"
	}
}
